<h1>{{title}}</h1>
<p>Welcome Ironhacker!</p>

<a href="/userProfile">Edit Profile</a>

<div class="post-list">
  {{#each posts}}
  <a href="/post/{{ _id }}">
  <span>Created by {{ creator.name }}</span>
  <div class="post">
    <p>{{ content }}</p>
  </div>
</a>
  {{/each}}
</div>

 <a href="/create">Create post</a>